<link rel="stylesheet" href="@Url.Content("~/Content/styles/table.css")">
<table class="tab">
    <tr>
        <td><p>Фамилия</p></td>
        <td><p>Имя</p></td>
    </tr>
    @foreach (var s in ViewBag.Students)
    {
        <tr>
            <td><p>@s.firstname</p></td>
            <td><p>@s.lastname</p></td>
        </tr>
    }
</table>

